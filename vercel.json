{
  "functions": {
    "pages/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "installCommand": "npm install --include=optional",
  "buildCommand": "npm run build"
}
