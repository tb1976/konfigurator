{
  "functions": {
    "pages/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install --omit=optional",
  "env": {
    "NODE_ENV": "production"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/",
      "permanent": false
    }
  ]
}
